  },
  "bin": {
    "wpmigrate": "cli.js"
  },
  "keywords": [],
  "author": {
    "name": "Matthieu COLLE",
    "url": "https://github.com/raiseandfall"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raiseandfall/wpmigrate/issues"
  },
  "homepage": "https://github.com/raiseandfall/wpmigrate",
  "devDependencies": {
    "jshint": "^2.6.3",
    "jshint-stylish": "^1.0.1",
    "mocha": "*",
    "watchify": "^3.1.0"
  },
  "dependencies": {
    "cli": "^0.6.6",
    "dateformat": "^1.0.11",
    "exit": "^0.1.2",
    "fs-extra": "^0.18.3",
    "lodash": "^3.9.1",
    "meow": "^3.1.0",
    "shelljs": "^0.5.0",
    "strip-json-comments": "^1.0.2"
